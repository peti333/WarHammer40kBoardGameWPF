<Window x:Class="Warhammer40kBoardGame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Warhammer40kBoardGame"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="StartButton" Content="Start" HorizontalAlignment="Center" Margin="0,135,0,0" VerticalAlignment="Top" Height="82" Width="162" Click="StartButton_Click"/>
        <Button x:Name="Inventory" Content="Inventory" HorizontalAlignment="Center" Margin="0,262,0,0" VerticalAlignment="Top" Height="79" Width="162" Click="Inventory_Click"/>

    </Grid>
</Window>
